{
  "name": "lint-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && nest build user && nest build post && nest build relationship && nest build interaction && nest build notification && nest build auth",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prisma:install:user": "npx prisma migrate dev --schema prisma/user/user-schema.prisma",
    "prisma:install:post": "npx prisma migrate dev --schema prisma/post/post-schema.prisma",
    "prisma:install:relationship": "npx prisma db push --schema=./prisma/relationship/relationship-schema.prisma",
    "prisma:install:interaction": "npx prisma db push --schema=./prisma/interaction/interaction-schema.prisma",
    "prisma:install:notification": "npx prisma db push --schema=./prisma/notification/notification-schema.prisma",
    "prisma:install": "yarn prisma:install:user && yarn prisma:install:post && yarn prisma:install:relationship && yarn prisma:install:interaction && yarn prisma:install:notification",
    "prisma:user": "npx prisma generate --schema prisma/user/user-schema.prisma",
    "prisma:relationship": "npx prisma generate --schema prisma/relationship/relationship-schema.prisma",
    "prisma:post": "npx prisma generate --schema prisma/post/post-schema.prisma",
    "prisma:interaction": "npx prisma generate --schema prisma/interaction/interaction-schema.prisma",
    "prisma:notification": "npx prisma generate --schema prisma/notification/notification-schema.prisma",
    "prisma:all": "yarn run prisma:user && yarn run prisma:relationship && yarn run prisma:post && yarn run prisma:interaction && yarn run prisma:notification",
    "protoc:user": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/user.proto",
    "protoc:auth": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/auth.proto",
    "protoc:relationship": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/following.proto ./proto/blacklist.proto",
    "protoc:post": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/post.proto",
    "protoc:interaction": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/like.proto ./proto/comment.proto",
    "protoc:notification": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./libs/common/src/types --ts_proto_opt=nestJs=true --proto_path=./proto ./proto/notification.proto",
    "protoc:all": "yarn protoc:user && yarn protoc:auth && yarn protoc:relationship && yarn protoc:post && yarn protoc:interaction && yarn protoc:notification",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "lint": "eslint \"{src,apps, libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.9",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.15.0",
    "aws-sdk": "^2.1659.0",
    "bcrypt": "^5.1.1",
    "isemail": "^3.2.0",
    "kafkajs": "^2.2.4",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-grpc-exceptions": "^0.2.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "superstruct": "^1.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "5.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.180.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
